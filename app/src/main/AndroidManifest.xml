<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andrewsapp.accstore2">

    <uses-permission android:name="android.permission.USE_BIOMETRIC" />
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name="com.andrewsapp.accstore2.Edit"
            android:screenOrientation="nosensor"/>
        <activity
            android:name="com.andrewsapp.accstore2.EntryView"
            android:screenOrientation="nosensor"/>
        <activity
            android:name="com.andrewsapp.accstore2.Settings"
            android:screenOrientation="nosensor"/>
        <activity
            android:name="com.andrewsapp.accstore2.Createaccount"
            android:screenOrientation="nosensor" />
        <activity
            android:name="com.andrewsapp.accstore2.FingerprintActivity"
            android:screenOrientation="nosensor" />
        <activity
            android:name="com.andrewsapp.accstore2.ForgotPassword"
            android:screenOrientation="nosensor" />
        <activity
            android:name="com.andrewsapp.accstore2.Accounts"
            android:screenOrientation="nosensor" />
        <activity
            android:name="com.andrewsapp.accstore2.Login"
            android:screenOrientation="nosensor" />
        <activity
            android:name="com.andrewsapp.accstore2.OneTime"
            android:screenOrientation="nosensor" />
        <activity
            android:name="com.andrewsapp.accstore2.MainActivity"
            android:screenOrientation="nosensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>